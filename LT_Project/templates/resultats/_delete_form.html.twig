<form method="post" action="{{ path('resultats_delete', {'id': resultat.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ resultat.id) }}">
    <button  style="border: solid 2px red; margin-top: 10px;">
    	<i class="fas fa-trash-alt red-text"></i></button>
</form>
