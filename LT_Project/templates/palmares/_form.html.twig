 {{ form_start(form) }}
    {{ form_errors(form) }}
    <table class="table table-striped">
    	<tbody>
		  	<tr class="text-center">
		  		<th>Nom : <br> {{ form_widget(form.name, { 'attr': {'class': "span12",'placeholder': "Nom de la comp√©tition", 'cols' : '100'} }) }}</th>
		  	</tr>
		  	<tr class="text-center">
		  		<th>Rank : <br> {{ form_widget(form.rank, { 'attr': {'class': "span12", 'cols' : '100'} }) }}</th>
		  	</tr>
		  	<tr class="text-center">
		  		<th>Date : <br> {{ form_widget(form.date, { 'attr': {'class': "span12"} }) }}</th>
		  	</tr>
		  	<tr class="text-center">
		  		<th>Liste des jeux : <br> {{ form_widget(form.jeux, { 'attr': {'class': "span12"} }) }}</th>
		  	</tr>
		  	<tr class="text-center">
		  		<th>Line up : <br> {{ form_widget(form.lineup, { 'attr': {'class': "span12", 'cols' : '100'} }) }}</th>
		  	</tr>
		  	<tr class="text-center">
		  		<th>Liste des joueur : <br> {{ form_widget(form.user, { 'attr': {'class': "span12"} }) }}</th>
		  	</tr>
		</tbody>
	</table>
	<div class="text-center">
		<button class="text-center btn mb-5" style="background-color: #63c7d2;">{{ button_label|default('Valider') }}</button>
    </div>
{{ form_end(form) }}