<form method="post" action="{{ path('lineup_delete', {'id': lineup.id}) }}" onsubmit="return confirm('Êtes vous sûrs ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ lineup.id) }}">
    <button  class="delete-btn">
    	<i class="fas fa-trash-alt red-text"></i></button>
</form>
